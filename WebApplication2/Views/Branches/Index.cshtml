@model IEnumerable<Branches>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Создать", "Create")
</p>
<table class="table">
    <tr>
        <th>
            Номер ветви
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.BranchNumber)
        </td>
        <td>
            @Html.ActionLink("Изменить", "Edit", new { id=item.idBranch }) |
            @Html.ActionLink("Детали", "Details", new { id=item.idBranch }) |
            @Html.ActionLink("Удалить", "Delete", new { id=item.idBranch })
        </td>
    </tr>
}

</table>
